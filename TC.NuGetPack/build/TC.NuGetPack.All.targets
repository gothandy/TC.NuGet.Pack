<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="TCNuGetPackAllTarget" AfterTargets="Build">
    <!--<Exec Command="$(ProjectDir)NuGetPack.bat $(ProjectDir)" />-->
    <!--<Exec Command="$(NuGetExePath) spec" />-->
    <Exec Command="for %%f in ($(ProjectDir)*.nuspec) do $(NuGetExePath) pack %%f" />
    <Exec Command="move $(ProjectDir)*.nupkg $(SolutionDir).nuget\packages" />
  </Target>
</Project>